{
  "name": "jiho",
  "version": "1.13.3",
  "private": false,
  "type": "module",
  "homepage": "https://uosjudo.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/uos-judo-jiho"
  },
  "scripts": {
    "clean": "rm -rf build",
    "clean:dev": "npm run clean && rm -rf public/prerendered",
    "build": "sh script/build.sh",
    "build:client": "vite build --outDir build/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir build/server",
    "build:server-ts": "tsc -p tsconfig.server.json",
    "build:prerender": "node script/prerender.js",
    "build:docker": "npm run clean && sh script/docker-build-local.sh",
    "preview": "cross-env NODE_ENV=production tsx server/index.ts",
    "start": "npm run preview",
    "start:prod": "node build/server-ts/index.js",
    "start:docker": "sh script/docker-run-local.sh",
    "orval": "orval",
    "orval:watch": "orval --watch",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc -p tsconfig.server.json --noEmit",
    "dev": "vite",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts --local"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.66.11",
    "@uiw/react-markdown-preview": "^5.1.5",
    "@uiw/react-md-editor": "^4.0.8",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "http-proxy-middleware": "^2.0.7",
    "ignore-styles": "^5.0.1",
    "lucide-react": "^0.477.0",
    "multer": "^2.0.2",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.1",
    "react": "^18.2.0",
    "react-cookie": "^7.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sirv": "^3.0.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^4.9.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@tailwindcss/postcss": "^4.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.19.78",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-import": "^1.13.6",
    "cross-env": "^10.0.0",
    "orval": "^8.2.0",
    "prettier": "^3.7.4",
    "serve": "^14.2.4",
    "tsx": "^4.20.6",
    "vite": "^6.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://uosjudo.com"
}
